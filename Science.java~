import java.util.ArrayList;
import java.util.List;

public class Science {

  public static void main (String[] args){
    Int[] nums= new int[]{1,2,3,4,5,6,7};
    List<Integer>list=Arrays.asList(nums);
    for (int=0; i<list.size();i++){
      if(list.get(i)==1){
        list.remove(i);
      }
    }
    
  
  }

}